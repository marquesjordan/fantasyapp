<h1>HOME PAGE</h1>

<div class="container">
	<div class="row">
		<div class="col-md-6">
			<table>
				<tr>
					<th>Name</th>
					<th>Position</th>
					<th>Select</th>
				</tr>
				<tr ng-repeat="player in players" class="rows">
					<td>{{player.name}}</td>
					<td>{{player.position}}</td>
					<td><button ng-click="picks(player, $index)">+</button>
				</tr>
			</table>
		
		</div>
		<div class="col-md-6">
			
		</div>
	</div>
</div>