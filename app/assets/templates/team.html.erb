<div class="container" id="body_main">
	<h1>Teams</h1>
	<br>
	
	<div class="contest_wrapper">

		<div class="contest_wrapper">
			<table class="table table-bordered table-striped table-fixed-header">
				<thead class="header">
				<tr>
					<th>Username</th>
					<th>Point Gaurd</th>
					<th>Shooting Gaurd</th>
					<th>Small Forward</th>
					<th>Power Forward</th>
					<th>Center</th>
					<th>6th Man</th>
					<th>Score</th>
				</tr>
				</thead>
				<tbody>
				<tr ng-repeat="team in gameEntry">
					<td>{{team.nickname}}</td>
					<td>{{team.pg_name}}</td>
					<td>{{team.sg_name}}</td>
					<td>{{team.sf_name}}</td>
					<td>{{team.pf_name}}</td>
					<td>{{team.c_name}}</td>
					<td>{{team.sixth_name}}</td>
					<td>{{team.fan_points}}</td>
				</tr>
				</tbody>
			</table>

		</div>

	</div>

</div>